<div>
  <h1>Products</h1>
  <table>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
    </tr>
    {{#each product}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.price}}</td>
        </tr>
    {{/each}}
  </table>
</div>